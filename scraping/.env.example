# Server Configuration (optional for health checks)
SERVER_HOST="localhost"
SERVER_PORT="3000"

# Database Configuration
DATABASE_URL="postgresql://postgres:dev@127.0.0.1:5432/polymarket-scraper?schema=public"

# Polymarket API Endpoints (optional, defaults provided)
# POLYMARKET_DATA_ENDPOINT="https://data-api.polymarket.com"
# POLYMARKET_GAMMA_ENDPOINT="https://gamma-api.polymarket.com"

# Scraper Configuration (optional, defaults provided)
# MARKETS_BATCH_SIZE="100"
